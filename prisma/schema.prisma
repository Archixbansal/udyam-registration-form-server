// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Submission {
  id               String @id @default(uuid())
  aadhaar          String
  name             String
  organizationType String
  pan              String
  createdAt        DateTime @default(now())
}